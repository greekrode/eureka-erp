{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@react/*": [
        "src/components/react/*"
      ],
      "@ui/*": [
        "src/components/ui/*"
      ],
      "@aceternityui/*": [
        "src/components/react/aceternityui/*"
      ],
      "@magicui/*": [
        "src/components/react/magicui/*"
      ]
    }
  }
}